<div id="l-top" class="c-top">
  <div class="c-top__container">
    <div class="c-top__container-wrap">
      <h1 class="c-top__subject">
        <%= @user.name %>のタグ一覧
      </h1>
    </div>
  </div>

  <div class="c-top__container">
    <div class="c-top__container-wrap">
      <ul class="c-top__bar c-bread">
        <li class="c-bread__item" itemscope="" itemtype="http://data-vocabulary.org/Breadcrumb">
          <a href="/" class="c-bread__item-link" itemprop="url">
            <span class="c-bread__item-label" itemprop="title">部屋の実例</span>
          </a>
        </li>
        <li class="c-bread__item" itemscope="" itemtype="http://data-vocabulary.org/Breadcrumb">
          <a href="/myroom/3429048" class="c-bread__item-link" itemprop="url">
            <span class="c-bread__item-label" itemprop="title"><%= @user.name %>のお部屋</span>
          </a>
        </li>
        <li class="c-bread__item" itemscope="" itemtype="http://data-vocabulary.org/Breadcrumb">
          <a href="/mytag/3429048" class="c-bread__item-link" itemprop="url">
            <span class="c-bread__item-label" itemprop="title"><%= @user.name %>のタグ一覧</span>
          </a>
        </li>
      </ul>
    </div>
  </div>
</div>

<div class="container">
  <article>
    <ul class="tag-list">
     <% @tags.each do |tag| %>
      <li id="tid2">
        <a href="/myphoto/3429048/?tag_id=2" target="_blank">
          <span><%= tag %></span>
          <span>(<%= tag.taggings_count %>)</span>
        </a>
      </li>
    <% end %>
</ul>
  </article>
</div>
